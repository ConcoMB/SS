.well
  #progress-box.clearfix
    Progress:
    .progress.progress-striped
      #progress.progress-bar.progress-bar-success{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "40", role: "progressbar", style: "width: 40%"}
        %span.sr-only 40% Complete (success)
  #speed-controls
    #speed-slider
    .speed
      Speed: 
      #speed.label.label-default= 1
  Time:
  #time.label.label-info= 0
  #results{:'data-url' => state_simulation_path(@simulation), :'data-total-packets' => @simulation.total_packets}
    #sender.panel.panel-default
      .panel-heading Sender
      .panel-body
        .router
    #receiver.panel.panel-default
      .panel-heading Receiver
      .panel-body
        .router